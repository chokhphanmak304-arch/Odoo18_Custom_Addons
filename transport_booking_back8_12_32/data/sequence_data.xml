<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence สำหรับเลขที่จองคิวรถขนส่ง -->
        <record id="sequence_vehicle_booking" model="ir.sequence">
            <field name="name">Vehicle Booking Sequence</field>
            <field name="code">vehicle.booking</field>
            <field name="prefix">BOOK-%(year)s%(month)s%(day)s-</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
        </record>
    </data>
</odoo>
