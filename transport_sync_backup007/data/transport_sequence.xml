<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence สำหรับ Transport Order -->
        <record id="seq_transport_order" model="ir.sequence">
            <field name="name">Transport Order Sequence</field>
            <field name="code">transport.order</field>
            <field name="prefix">TR-%(year)s%(month)s%(day)s-</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>