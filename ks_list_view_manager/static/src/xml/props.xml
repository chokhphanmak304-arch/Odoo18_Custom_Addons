<?xml version="1.0" encoding="UTF-8" ?>
<template>
    <t
        t-name="ks_view_manager.Listcontroller"
        t-inherit="web.ListView"
        t-inherit-mode="extension"
        owl="1">

        <xpath expr="//t[@t-component='props.Renderer']" position="attributes">
            <attribute name="Ks_update_field_data.bind">ks_update_field_data</attribute>
            <attribute name="Ks_initialize_lvm_data.bind">ks_initialize_lvm_data</attribute>
            <attribute name="list_data">list_data</attribute>

        </xpath>
    </t>
    <t t-name="ks_view_manager.CheckBox" t-inherit="web.CheckBox" t-inherit-mode="extension" owl="1">
           <xpath expr="//div[hasclass('form-check')]" position="attributes">
            <attribute name="class">o-checkbox form-check d-flex m-0 lvm_checkbox</attribute>
        </xpath>
    </t>
</template>