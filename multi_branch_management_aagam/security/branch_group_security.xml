<?xml version="1.0"?>
<odoo>
    <data noupdate="1">

        <!--
            Users Groups
            Note that the field 'category_id' is set later in
            base/data/ir_module_category_data.xml
        -->
        <!--   For multi brnach   -->
        <record model="res.groups" id="group_multi_branch">
            <field name="name">Multi Branch</field>
            <field name="category_id" ref="base.module_category_usability"/>
            <field name="users" eval="[(4, ref('base.user_admin'))]"/>
        </record>

        <record id="group_branch_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="multi_branch_management_aagam.module_category_branches_role"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="group_branch_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="multi_branch_management_aagam.module_category_branches_role"/>
            <field name="implied_ids" eval="[(4, ref('group_branch_user'))]"/>
        </record>

    </data>
</odoo>
